{
  "name": "n8n-nodes-sendseven",
  "version": "1.0.2",
  "description": "n8n community node for SendSeven - Unified Messaging API Platform",
  "keywords": [
    "n8n-community-node-package",
    "sendseven",
    "messaging",
    "whatsapp",
    "telegram",
    "sms",
    "email"
  ],
  "license": "MIT",
  "homepage": "https://sendseven.com",
  "author": {
    "name": "SendSeven GmbH",
    "email": "dev@sendseven.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SendSeven-GmbH/n8n-nodes-sendseven"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SendSevenApi.credentials.js",
      "dist/credentials/SendSevenOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/SendSeven/SendSeven.node.js",
      "dist/nodes/SendSeven/SendSevenTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.20.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
